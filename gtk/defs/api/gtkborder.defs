
(define-function gtk_border_get_type
  (in-module "GtkBorder")
  (c-name "gtk_border_get_type")
  (return-type "GType")
)

(define-method copy
  (of-object "GtkBorder")
  (c-name "gtk_border_copy")
  (return-type "GtkBorder*")
  (caller-owns-return #t)
)

(define-method free
  (of-object "GtkBorder")
  (c-name "gtk_border_free")
  (return-type "none")
)

