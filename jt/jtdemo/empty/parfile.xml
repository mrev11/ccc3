<xstart>

<item>
    <name>Empty Demó SSL nélkül</name>
    <host>localhost</host>
    <port>46000</port>
    <workdir></workdir>
    <command>empty.exe -jtsocket $(SOCKET)</command>
</item>

<item>
    <name>Empty Demó SSL forwarding-gal</name>
    <host>localhost</host>
    <port>46001</port>
    <workdir></workdir>
    <command>sslforward.exe empty.exe -jtsocket $(SOCKET)</command>
</item>


<item>
    <name>Empty Demó beépített SSL-lel</name>
    <host>localhost</host>
    <port>46002</port>
    <workdir></workdir>
    <command>empty.exe 
                -jtsocket $(SOCKET) 
                -jtkey localhost.pem
    </command>
</item>

<item>
    <name>Empty Demó beépített SSL-lel, kliens hitelesítés</name>
    <host>localhost</host>
    <port>46003</port>
    <workdir></workdir>
    <command>empty.exe  
                -jtsocket $(SOCKET)
                -jtkey localhost.pem
                -jtca localhost.pem
    </command>
</item>
 
</xstart>
 