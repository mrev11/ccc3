

******************************************************************************************
function main(bg:="1",fg:="0")


    ? chr(27)+"[38;5;"+fg+"m"+chr(27)+"[48;5;"+bg+"m------------------------"
    ?? chr(27)+"[m "
    ?? ansi_colors(bg::val+1)::rgb
    
    ?
	inkey(0)	

******************************************************************************************
static function rgb(rgb)
local x:=" ["

    x+=" "+rgb[1]::l2hex::padl(2,"0")
    x+=" "+rgb[2]::l2hex::padl(2,"0")
    x+=" "+rgb[3]::l2hex::padl(2,"0")
    x+=" ] "
    
    return x

