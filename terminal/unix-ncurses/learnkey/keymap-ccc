string F20 = "\033[34~"
string Find = "\033[1~"
string Insert = "\033[2~"
string Remove = "\033[3~"
string Select = "\033[4~"
string Prior = "\033[5~"
string Next = "\033[6~"
string Macro = "\033[M"
string Pause = "\033[P"

#alt-arrow
string F51 = "\033[1;3F" #! end
string F52 = "\033[1;3B" #! down
string F53 = "\033[6;3~" #! pgdn
string F54 = "\033[1;3D" #! left
string F56 = "\033[1;3C" #! right
string F57 = "\033[1;3H" #! home
string F58 = "\033[1;3A" #! up
string F59 = "\033[5;3~" #! pgup

#ctl-arrow
string F61 = "\033[1;5F" #!
string F62 = "\033[1;5B" #!
string F63 = "\033[6;5~" #!
string F64 = "\033[1;5D" #!
string F66 = "\033[1;5C" #!
string F67 = "\033[1;5H" #!
string F68 = "\033[1;5A" #!
string F69 = "\033[5;5~" #!

