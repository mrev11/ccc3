
iface=127.0.0.1             # interface to listen (default: localhost)
port=45678                  # port to listen (default: 45678)
ssl=false                   # use SSL (default: true)
keyfile=keys/webapp.pem     # private key (nincs default)
# capath=keys
# cafile=keys/webapp.pem
gzip=true                   # content-encoding (default: true)
permessage-deflate=true     # sec-websocket-extensions  (default: true)
exec=wstart.exe             # webapp to start (also use "webapp_exec")
